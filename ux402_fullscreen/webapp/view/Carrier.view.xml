<mvc:View xmlns:table="sap.ui.table" controllerName="student31.com.sap.training.ux402.fullscreen.ux402fullscreen.controller.Carrier"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>overviewPageTitle}">
        <content>
            <Table id="_IDGenTable1" items="{/CarrierCollection}">
                <columns>
                    <Column minScreenWidth="Tablet" id="_IDGenColumn1">
                    <Text id="_IDGenText1" text="{i18n>columnId}"></Text>
                    </Column>
                    <Column minScreenWidth="Tablet" id="_IDGenColumn2">
                    <Text id="_IDGenText2" text="{i18n>columnName}"></Text>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1" press="onPress" type="Navigation">
                        <cells>
                            <ObjectIdentifier id="_IDGenObjectIdentifier1" title="{carrid}"></ObjectIdentifier>
                            <Text id="_IDGenText3" text="{CARRNAME}"></Text>
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </content>
    </Page>
</mvc:View>
